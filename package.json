{
  "name": "my-cv",
  "version": "0.1.0",
  "description": "MEVN fullstack app",
  "author": "Kostiantin Peschanov (constantine.peschanov@gmail.com)",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:constantinesimm/my-cv.git"
  },
  "main": "server.js",
  "scripts": {
    "start": "node server/bin/www",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev:app": "concurrently \"npm run serve\" \"npm run dev:server\"",
    "dev:server": "nodemon server/bin/www",
    "dev:static": "npm run build && npm run dev:server",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "ajv": "^6.12.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "epic-spinners": "^1.1.0",
    "express": "~4.16.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.7",
    "morgan": "~1.9.1",
    "nodemailer": "^6.4.6",
    "puppeteer": "^2.1.1",
    "shards-ui": "^3.0.0",
    "shards-vue": "^1.0.7",
    "vue": "^2.6.11",
    "vue-dropdowns": "^1.1.1",
    "vue-i18n": "^8.16.0",
    "vue-router": "^3.1.6",
    "vue-toast-notification": "^0.2.0",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "nodemon": "^2.0.3",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "MongoDB",
    "ExpressJS",
    "VueJS",
    "NodeJS"
  ]
}
